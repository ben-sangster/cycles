<?xml version="1.0" encoding="utf-8"?>
<dmz>
   <plugin-list>
      <plugin name="dmzAudioModuleOpenAL" platform="macos|linux"/>
      <plugin name="dmzAudioModuleFMOD" platform="win32"/>
      <plugin name="dmzAudioModulePortalBasic"/>
      <plugin name="dmzAudioPluginObject" />
      <plugin name="dmzAudioPluginEvent" />
   </plugin-list>
   <dmzAudioModulePortalBasic>
     <name value="audioportal1"/>
   </dmzAudioModulePortalBasic>
   <dmzAudioModuleFMOD>
      <dopplerScale value="1.0"/>
      <distanceFactor value="1.0"/>
      <rollOffScale value="1.0"/>
      <maxChannels value="256"/>
      <rightHandedCoordinates value="True"/>
      <forceFullDirectSoundHRTF value="True"/>
   </dmzAudioModuleFMOD>
   <dmzAudioPluginEvent>
      <event-list>
         <event-type name="Event_Collision"/>
      </event-list>
   </dmzAudioPluginEvent>
</dmz>
